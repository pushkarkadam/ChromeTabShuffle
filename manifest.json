{
    "name": "ChromeTabShuffle",
    "version": "0.0.1",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches" :["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "tabs"
    ]
}
